{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "filament/forms": "^3.2.130",
        "filament/tables": "^3.2.130",
        "laravel/breeze": "^2.3.2",
        "laravel/framework": "^11.40.0",
        "laravel/tinker": "^2.10",
        "livewire/livewire": "^3.5.18",
        "spatie/laravel-cookie-consent": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5.4",
        "brianium/paratest": "^7.7",
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.0.2",
        "laravel/pail": "^1.2.1",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.40.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.6.1",
        "pestphp/pest": "^3.7.4",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-livewire": "^3.0",
        "pestphp/pest-plugin-type-coverage": "^3.2.3",
        "rector/rector": "^2.0.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "analyse": [
            "@php -d memory_limit=-1 ./vendor/bin/phpstan analyse --configuration=phpstan.neon"
        ],
        "pest": "./vendor/bin/pest",
        "lint": "./vendor/bin/pint",
        "refactor": "./vendor/bin/rector",
        "test:lint": "./vendor/bin/pint --test",
        "test:refector": "./vendor/bin/rector --dry-run",
        "test:coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "./vendor/bin/pest --parallel --coverage --exactly=79.1"
        ],
        "test:coverage-html": [
            "@putenv XDEBUG_MODE=coverage",
            "@php -d xdebug.mode=coverage vendor/bin/pest --coverage-html coverage --parallel"
        ],
        "type-coverage": "./vendor/bin/pest --type-coverage --min=97.8",
        "test:type-coverage": "./vendor/bin/pest --type-coverage --min=97.8",
        "test:parallel": "./vendor/bin/pest --parallel",
        "test:php-stan": "@analyse",
        "test": [
            "@test:refector",
            "@test:lint",
            "@test:type-coverage",
            "@test:php-stan",
            "@test:coverage"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
